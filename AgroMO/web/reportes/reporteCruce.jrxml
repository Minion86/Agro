	<parameter name="ID_CONTRATO" class="java.lang.Integer"/>
	<parameter name="CONTRACTUAL" class="java.lang.String">
		<defaultValueExpression><![CDATA["Las partes se ratifican y aceptan el contenido de las cláusulas contractuales."]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     c_contrato."c_contrato_id" AS c_contrato_id,
     c_contrato."c_empleado_id" AS c_empleado_id,
     c_contrato."c_institucion_id" AS c_institucion_id,
     c_contrato."c_tipo_contrato_id" AS c_tipo_contrato_id,
     c_contrato."fecha_inicio" AS fecha_inicio,
     c_contrato."fecha_fin" AS fecha_fin,
     c_grupo_ocupacional."nombre" AS c_grupo_ocupacional_nombre,
     c_contrato."cargo" AS cargo,
     c_contrato."remuneracion" AS remuneracion,
     c_contrato."periodo_prueba" AS periodo_prueba,
     c_contrato."jornada_nocturna" AS jornada_nocturna,
     c_contrato."funciones_confianza" AS funciones_confianza,
     c_contrato."joranda_mensual" AS joranda_mensual,
     c_contrato."jornada_especial" AS jornada_especial,
     c_contrato."codigo" AS c_contrato_codigo,
     c_empleado."nombre" AS c_empleado_nombre,
     "contratos".c_edad_empleado(c_contrato."c_empleado_id") as edad_empleado,
     c_empleado."fecha_nacimiento" AS c_empleado_fecha_nacimiento,
     c_empleado."identificacion" AS c_empleado_identificacion,
     c_empleado."extranjero" AS c_empleado_extranjero,
     c_empleado."genero" AS c_empleado_genero,
     c_empleado."discapcidad" AS c_empleado_discapcidad,
     c_empleado."tipo_discapacidad" AS c_empleado_tipo_discapcidad,
    case
        when c_empleado."tipo_discapacidad" ='A' THEN 'Auditiva'
        when c_empleado."tipo_discapacidad" ='F' THEN 'Física'
        when c_empleado."tipo_discapacidad" ='L' THEN 'Lenguaje'
        when upper(c_empleado."tipo_discapacidad") ='P' THEN 'Psicológica'
        when c_empleado."tipo_discapacidad" ='V' THEN 'Visual'
        when c_empleado."tipo_discapacidad" ='I' THEN 'Intelectual'
        else 'Ninguna'
    end as tipo_discapcidad,
     c_empleado."ex_migrante" AS c_empleado_ex_migrante,
     c_tipo_contrato."nombre" AS c_tipo_contrato_nombre,
     c_institucion."razon_social" AS c_institucion_razon_social,
     c_institucion."identificacion" AS c_institucion_identificacion,
     "contratos".c_representante_lega(c_contrato."c_institucion_id") as representante_nombre,
      c_contrato."es_empleo_joven" AS es_empleo_joven
FROM
     "contratos"."c_empleado" c_empleado INNER JOIN "contratos"."c_contrato" c_contrato ON c_empleado."c_empleado_id" = c_contrato."c_empleado_id"
     INNER JOIN "contratos"."c_tipo_contrato" c_tipo_contrato ON c_contrato."c_tipo_contrato_id" = c_tipo_contrato."c_tipo_contrato_id"
     INNER JOIN "contratos"."c_institucion" c_institucion ON c_contrato."c_institucion_id" = c_institucion."c_institucion_id"
     INNER JOIN "contratos"."c_grupo_ocupacional" c_grupo_ocupacional ON c_contrato."c_grupo_ocupacional_id" = c_grupo_ocupacional."c_grupo_ocupacional_id"
where c_contrato.c_contrato_id=$P{ID_CONTRATO}]]>
	</queryString>
